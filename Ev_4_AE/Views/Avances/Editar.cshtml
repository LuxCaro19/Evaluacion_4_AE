@model Ev_4_AE.Models.AvanceOrdenTrabajo
@{
    ViewBag.Title = "Editar info de avance";
    Layout = "~/Views/Shared/_Interna.cshtml";
}

<br />
<div class="card">
    <div class="card-header">
        <h5 class="text-center">Nuevo</h5>
    </div>
    <div class="card-body">
        <h5 class="card-title">Completar con la información</h5>
        @using (Html.BeginForm("Editar", "Avances", FormMethod.Post))
        {
            @Html.Partial("_formularios", Model)
        }
    </div>
</div>
